{
  "expo": {
    "name": "ubelloch-tasks",
    "slug": "ubelloch-tasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ubellochtasks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ubellochtasks"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ubellochtasks"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Fredoka-Bold.ttf",
            "./assets/fonts/Fredoka-Light.ttf",
            "./assets/fonts/Fredoka-Medium.ttf",
            "./assets/fonts/Fredoka-Regular.ttf",
            "./assets/fonts/Fredoka-SemiBold.ttf",

            "./assets/fonts/albert-sans-latin-400-normal.ttf",
            "./assets/fonts/albert-sans-latin-400-italic.ttf",
            "./assets/fonts/albert-sans-latin-500-normal.ttf",
            "./assets/fonts/albert-sans-latin-500-italic.ttf",
            "./assets/fonts/albert-sans-latin-600-normal.ttf",
            "./assets/fonts/albert-sans-latin-600-italic.ttf",
            "./assets/fonts/albert-sans-latin-700-normal.ttf",
            "./assets/fonts/albert-sans-latin-700-italic.ttf",
            "./assets/fonts/albert-sans-latin-800-normal.ttf",
            "./assets/fonts/albert-sans-latin-800-italic.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
