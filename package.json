{
  "name": "momentum",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --dev-client --reset-cache",
    "start": "expo start --dev-client",
    "start:clear": "expo start --reset-cache --dev-client",
    "android": "expo run:android",
    "update": "npx expo install --fix",
    "build": "npx expo prebuild",
    "build:eas": "npx eas build --platform android --profile development",
    "build:local": "eas build --platform android --profile development --local",
    "build:clear": "npx expo prebuild --clean",
    "db:generate": "npx drizzle-kit generate",
    "tree": "tree -L 3 -I \"node_modules|android|ios|vscode|.git|.idea|dist|build\"  --dirsfirst",
    "lint": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx,json}\" && eslint . && tsc --noEmit --listFilesOnly -p ./tsconfig.json"
  },
